<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.11.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="VIP">
        <createTable tableName="VIP_NOTIFICATION">
            <column name="ID" type="UUID">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_VIP_NOTIFICATION"/>
            </column>
            <column name="VERSION" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="CREATED_BY" type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="DATETIME"/>
            <column name="LAST_MODIFIED_BY" type="VARCHAR(255)"/>
            <column name="LAST_MODIFIED_DATE" type="DATETIME"/>
            <column name="DELETED_BY" type="VARCHAR(255)"/>
            <column name="DELETED_DATE" type="DATETIME"/>
            <column name="CREATION_DATE" type="DATETIME"/>
            <column name="REMARKS" type="CLOB"/>
            <column name="SOURCE" type="VARCHAR(255)"/>
            <column name="NOTIFICATION_DATE" type="DATETIME"/>
            <column name="CHECK_NAME" type="VARCHAR(255)"/>
            <column name="LEVEL_" type="VARCHAR(255)"/>
            <column name="MESSAGE" type="VARCHAR(255)"/>
            <column name="ENTITY" type="VARCHAR(255)"/>
            <column name="GROUP_ID" type="INT"/>
            <column name="SYS_TENANT_ID" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>