<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.11.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="VIP">
        <createTable tableName="VIP_DATE_VALUE_VOLUME">
            <column name="id" type="UUID">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_VIP_DATE_VALUE_VOLUME"/>
            </column>
            <column name="date_" type="DATETIME">
                <constraints nullable="false"/>
            </column>
            <column name="value_" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="volume" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>